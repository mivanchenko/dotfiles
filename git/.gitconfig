[user]
	name = Michael Ivanchenko
#	email = 
[color]
	status = auto
	branch = auto
	interactive = auto
	diff = auto
[push]
	default = simple
[merge]
	ff = false
[alias]
	co = checkout
	ci = commit
	st = status
	br = branch
	logg = log --graph --abbrev-commit --decorate --all --format=format:\"%C(bold blue)%h%C(reset) %C(cyan)%ad%C(dim white) %an%C(reset) %C(green)(%ar)%C(reset)%C(bold yellow)%d%C(reset)%n %C(white)%s%C(reset)\" --date=short
	pf = push --force-with-lease origin
[core]
	whitespace = space-before-tab,trailing-space,cr-at-eol
	ignorecase = false
[diff]
	renames = copy
[pack]
	threads = 0
[credential]
	helper = osxkeychain
